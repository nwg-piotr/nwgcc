{
  "preferences": {
    "icon_set": "light",
    "custom_styling": false,
    "dont_close": false,
    "window_decorations": false,
    "show_cli_label": true,
    "show_brightness_slider": true,
    "show_volume_slider": true,
    "show_playerctl": false,
    "show_user_line": true,
    "show_wifi_line": true,
    "show_bt_line": true,
    "show_battery_line": true,
    "show_user_rows": true,
    "show_user_buttons": true,
    "icon_size_small": 16,
    "icon_size_large": 24,
    "refresh_fast_millis": 500,
    "refresh_slow_seconds": 5,
    "refresh_cli_seconds": 1800,
    "on-click-user": "",
    "on-click-wifi": "nm-connection-editor",
    "on-click-bluetooth": "blueman-manager",
    "on-click-battery": ""
  },
  "icons": {
    "battery-empty": "battery-empty-symbolic",
    "battery-low": "battery-low-symbolic",
    "battery-good": "battery-good-symbolic",
    "battery-full": "battery-full-symbolic",
    "user": "user-available-symbolic",
    "wifi-on": "network-wireless-connected-symbolic",
    "wifi-off": "network-wireless-disconnected-symbolic",
    "brightness-low": "display-brightness-low-symbolic",
    "brightness-medium": "display-brightness-medium-symbolic",
    "brightness-high": "display-brightness-high-symbolic",
    "bt-on": "bluetooth-active-symbolic",
    "bt-off": "bluetooth-disabled-symbolic",
    "volume-low": "audio-volume-low-symbolic",
    "volume-medium": "audio-volume-medium-symbolic",
    "volume-high": "audio-volume-high-symbolic",
    "volume-muted": "audio-volume-muted-symbolic",
    "media-playback-pause": "media-playback-pause-symbolic",
    "media-playback-start": "media-playback-start-symbolic",
    "media-playback-stop": "media-playback-stop-symbolic",
    "media-seek-backward": "media-seek-backward-symbolic",
    "media-seek-forward": "media-seek-forward-symbolic",
    "media-skip-backward": "media-skip-backward-symbolic",
    "media-skip-forward": "media-skip-forward-symbolic",
    "click-me": "pan-end-symbolic"
  },
  "commands": {
    "get_battery": "upower -i $(upower -e | grep BAT) | grep --color=never -E 'state|to\\\\ full|to\\\\ empty|percentage'",
    "get_battery_alt": "acpi",
    "get_bluetooth_name": "bluetoothctl show | grep Name",
    "get_bluetooth_status": "bluetoothctl show | grep Powered",
    "get_brightness": "light -G",
    "get_host": "uname -n",
    "get_ssid": "iwgetid -r",
    "get_user": "echo $USER",
    "get_volume_alt": "amixer sget Master",
    "set_brightness": "light -S",
    "set_volume_alt": "amixer sset Master",
    "systemctl": "systemctl",
    "playerctl": "playerctl"
  }
}